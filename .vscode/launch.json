{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Tests",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "sourceMaps": true,
      "runtimeArgs": ["--nolazy"],
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v8.9.4/bin/node",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "test"
      }
    }
  ]
}
